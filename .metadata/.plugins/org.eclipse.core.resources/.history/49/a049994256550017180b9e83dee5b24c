package com.santana.solid.bad;

public class LiskovExample {
	public void calculateArea(Rectangle rect) {
		rect.setHeight(2);
		rect.setWidth(4);
		assert rect.getArea() == 8 : printError("area", rect);
		assert rect.getHeight() == 2 : printError("length", rect);
		assert rect.getWidth() == 4 : printError("length", rect);
	}
	public static void main(String[] args) {
		
	}
}
